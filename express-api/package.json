{
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "build": "pnpm build:types && pnpm build:es",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "nodemon --watch src --ext ts --exec 'pnpm build:es && pnpm start'",
    "build:es": "esbuild src/node-server.ts --bundle --platform=node --target=node20 --outdir=out",
    "start": "node out/node-server.js"
  },
  "dependencies": {
    "@sentry/serverless": "^7.118.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.19.2",
    "fp-ts": "^2.16.9",
    "http-status": "^1.7.4",
    "io-ts": "^2.2.21",
    "jose": "^5.6.3",
    "lodash": "^4.17.21",
    "mongodb": "^6.8.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "ofetch": "^1.3.4",
    "serverless-http": "^3.2.0",
    "slugify": "^1.6.6",
    "typera-express": "^2.4.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "serverless": "^3.39.0",
    "serverless-esbuild": "^1.52.1",
    "serverless-offline": "^13.6.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "urllib": "^3.27.0"
  }
