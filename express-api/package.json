{
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "build": "yarn build:types && yarn build:es",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "nodemon --watch src --ext ts --exec 'yarn build:es && yarn start'",
    "build:es": "esbuild src/node-server.ts --bundle --platform=node --target=node18 --outdir=out",
    "start": "node out/node-server.js"
  },
  "dependencies": {
    "@sentry/serverless": "^7.55.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "fp-ts": "^2.16.0",
    "http-status": "^1.6.2",
    "io-ts": "^2.2.20",
    "jose": "^4.14.4",
    "lodash": "^4.17.21",
    "mongodb": "^5.6.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.6",
    "ofetch": "^1.1.1",
    "serverless-http": "^3.2.0",
    "slugify": "^1.6.6",
    "typera-express": "^2.4.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "esbuild": "^0.18.10",
    "jest": "^29.5.0",
    "serverless": "^3.32.2",
    "serverless-esbuild": "^1.45.1",
    "serverless-offline": "^12.0.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "urllib": "^3.17.1"
  },
  "packageManager": "yarn@3.6.0"
}
